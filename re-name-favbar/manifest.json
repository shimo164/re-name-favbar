{
	"manifest_version": 3,
	"name": "re:Name FavBar",
	"version": "0.1",
	"description": "Rename AWS service names of the favorite bar on the console",
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"options_page": "options.html",
	"permissions": [
		"activeTab",
		"scripting",
		"storage"
	],
	"host_permissions": [
		"*://*.console.aws.amazon.com/*"
	],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
